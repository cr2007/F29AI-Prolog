{
    "name": "Prolog Container",
    "customizations": {
        "vscode": {
            "extensions": [
				"rebornix.prolog",
				"vscode-icons-team.vscode-icons"
			]
        }
    },
    "initializeCommand": [
        "sudo apt-add-repository ppa:swi-prolog/stable",
        "sudo apt-get update",
        "sudo apt-get install swi-prolog"
    ]
}